{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "description",
      "required": true
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "blogs"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "blogs"
    },
    "image": {
      "type": "text",
      "required": true
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "shared.rich-text"
      ],
      "required": false
    }
  }
}
